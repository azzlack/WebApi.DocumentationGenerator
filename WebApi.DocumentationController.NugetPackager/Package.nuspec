<?xml version="1.0"?>
<package >
  <metadata>
    <id>WebApi.DocumentationController</id>
	<title>WebApi Documentation Site Generator</title>
    <version>1.0.1</version>
    <authors>Ove Andersen</authors>
    <owners>EyeCatch</owners>
    <projectUrl>http://www.eyecatch.no/projects/aspnet-webapi-documentation-pages-generator/</projectUrl>
    <iconUrl>http://www.eyecatch.no/favicon.ico</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Controller for automatically generating documentation pages for your ASP.NET Web API.
    </description>
    <releaseNotes>
      Updated to work with WebAPI RTM
    </releaseNotes>
    <copyright>Copyright EyeCatch 2012</copyright>
    <tags>webapi documentation route</tags>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="jQuery" version="1.7.2"/>
        <dependency id="Microsoft.AspNet.Mvc" version="4.0.20710.0"/>
        <dependency id="Microsoft.AspNet.WebApi" version="4.0.20710.0"/>
        <dependency id="AttributeRouting.Core.Web" version="2.5.0.0" />
        <dependency id="WebActivator" version="1.5.1"/>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="lib\**" target="lib" />
    <file src="tools\**" target="tools" />
    <file src="content\**" target="content" />
  </files>
</package>